@article{corbo1997,
  title = {Characterization of a Notochord-Specific Enhancer from the {{Brachyury}} Promoter Region of the Ascidian, {{Ciona}} Intestinalis},
  author = {Corbo, J. C. and Levine, M. and Zeller, R. W.},
  date = {1997-02-01},
