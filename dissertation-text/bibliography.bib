@article{Manning2012-ro,
	abstract = {Recent genome-wide association studies have described many loci
               implicated in type 2 diabetes (T2D) pathophysiology and beta-cell
               dysfunction but have contributed little to the understanding of
               the genetic basis of insulin resistance. We hypothesized that
               genes implicated in insulin resistance pathways might be
               uncovered by accounting for differences in body mass index (BMI)
               and potential interactions between BMI and genetic variants. We
               applied a joint meta-analysis approach to test associations with
               fasting insulin and glucose on a genome-wide scale. We present
               six previously unknown loci associated with fasting insulin at P
               < 5 × 10(-8) in combined discovery and follow-up analyses of 52
               studies comprising up to 96,496 non-diabetic individuals. Risk
               variants were associated with higher triglyceride and lower
               high-density lipoprotein (HDL) cholesterol levels, suggesting a
               role for these loci in insulin resistance pathways. The discovery
               of these loci will aid further characterization of the role of
               insulin resistance in T2D pathophysiology.},
