@article{ashley2016,
  title = {Intervertebral Disc Development and Disease-Related Genetic Polymorphisms},
  author = {Ashley, Jason W. and {Enomoto-Iwamoto}, Motomi and Smith, Lachlan J. and Mauck, Robert L. and Chan, Danny and Lee, Joseph and Heyworth, Martin F. and An, Howard and Zhang, Yejia},
  year = {2016},
  month = sep,
  journal = {Genes \& Diseases},
  volume = {3},
  number = {3},
