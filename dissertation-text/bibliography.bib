@article{ward2012,
  title = {Evidence of {{Abundant Purifying Selection}} in {{Humans}} for {{Recently Acquired Regulatory Functions}}},
  author = {Ward, Lucas D. and Kellis, Manolis},
  date = {2012-09-28},
  journaltitle = {Science},
  shortjournal = {Science},
  volume = {337},
  number = {6102},
