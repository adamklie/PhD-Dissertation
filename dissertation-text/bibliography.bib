@article{grossman2017,
  title = {Systematic Dissection of Genomic Features Determining Transcription Factor Binding and Enhancer Function},
  author = {Grossman, Sharon R. and Zhang, Xiaolan and Wang, Li and Engreitz, Jesse and Melnikov, Alexandre and Rogov, Peter and Tewhey, Ryan and Isakova, Alina and Deplancke, Bart and Bernstein, Bradley E. and Mikkelsen, Tarjei S. and Lander, Eric S.},
  year = {2017},
  month = feb,
  journal = {Proceedings of the National Academy of Sciences},
  volume = {114},
  number = {7},
  pages = {E1291-E1300},
  issn = {0027-8424, 1091-6490},
  doi = {10.1073/pnas.1621150114},
  abstract = {Enhancers regulate gene expression through the binding of sequence-specific transcription factors (TFs) to cognate motifs. Various features influence TF binding and enhancer function\textemdash including the chromatin state of the genomic locus, the affinities of the binding site, the activity of the bound TFs, and interactions among TFs. However, the precise nature and relative contributions of these features remain unclear. Here, we used massively parallel reporter assays (MPRAs) involving 32,115 natural and synthetic enhancers, together with high-throughput in vivo binding assays, to systematically dissect the contribution of each of these features to the binding and activity of genomic regulatory elements that contain motifs for PPAR{$\gamma$}, a TF that serves as a key regulator of adipogenesis. We show that distinct sets of features govern PPAR{$\gamma$} binding vs. enhancer activity. PPAR{$\gamma$} binding is largely governed by the affinity of the specific motif site and higher-order features of the larger genomic locus, such as chromatin accessibility. In contrast, the enhancer activity of PPAR{$\gamma$} binding sites depends on varying contributions from dozens of TFs in the immediate vicinity, including interactions between combinations of these TFs. Different pairs of motifs follow different interaction rules, including subadditive, additive, and superadditive interactions among specific classes of TFs, with both spatially constrained and flexible grammars. Our results provide a paradigm for the systematic characterization of the genomic features underlying regulatory elements, applicable to the design of synthetic regulatory elements or the interpretation of human genetic variation.},
  langid = {english}
}

