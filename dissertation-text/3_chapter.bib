,@ARTICLE{Gaulton2010-mq,
  title     = "A map of open chromatin in human pancreatic islets",
  author    = "Gaulton, Kyle J and Nammo, Takao and Pasquali, Lorenzo and Simon,
               Jeremy M and Giresi, Paul G and Fogarty, Marie P and Panhuis,
               Tami M and Mieczkowski, Piotr and Secchi, Antonio and Bosco,
